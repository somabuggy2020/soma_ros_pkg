<launch>
  <arg name="input_cloud_F" value="/camera_F/depth_registered/points" />
  <arg name="input_cloud_B" value="/camera_B/depth_registered/points" />
  <arg name="frame_id" value="soma_link"/>

  <node name="manager" pkg="nodelet" type="nodelet" args="manager">
  </node>

  <node name="tf_F" pkg="nodelet" type="nodelet" args="load jsk_pcl/TfTransformCloud manager">
    <remap from="~input" to="$(arg input_cloud_F)"/>
    <param name="target_frame_id" value="$(arg frame_id)"/>
  </node>

  <node name="tf_B" pkg="nodelet" type="nodelet" args="load jsk_pcl/TfTransformCloud manager">
    <remap from="~input" to="$(arg input_cloud_B)"/>
    <param name="target_frame_id" value="$(arg frame_id)"/>
  </node>

  <node name="marge_pcd" pkg="nodelet" type="nodelet" args="load soma_perception/marge_pcd manager">
    <remap from="/marge/input_cloud_1" to="tf_F/output"/>
    <remap from="/marge/input_cloud_2" to="tf_B/output"/>
  </node>

  

  <!-- for save PCD file -->
  <node name="save_pcd" pkg="nodelet" type="nodelet" args="load jsk_pcl_utils/PointCloudToPCD manager" output="screen">
    <remap from="~input" to="/marge/output"/>
    <param name="fixed_frame" value="$(arg frame_id)"/>
    <param name="prefix" value="/home/soma1/Documents/noboru/pcd-2021-01-20-14-52/"/>
  </node>

</launch>